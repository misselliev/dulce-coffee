{
  "parserOptions": { "ecmaVersion": 6 },
  "parser": "babel-eslint",
  "extends": "airbnb-base",
    rules: {
    'no-shadow': 0,
    'no-param-reassign': 0,
    'eol-last': 0,
    'no-use-before-define': ['error', { 'functions': false }],
    'new-cap': ["error", { "newIsCap": false }],
    'class-methods-use-this': 'warn'
  },
  "env": {
        "browser": true,
    },
}